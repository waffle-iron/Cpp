#include <stdio.h>

int main(void)
{
int size;
int k , l = 0;
printf("Введіть розмір елементів:");
scanf("%i",&size);
printf("Заповніть множину A:\n");
int A[500],B[500];
int x,y;
for (int i = 0; i < size; i++ )
A[i] = scanf("%i",&x);
printf("Заповніть множину B:\n");
for (int i = 0; i < size; i++)
B[i] = scanf("%i",&y);

for(int i = 0; i < size; i++){
l += A[i];
k += B[i];
}
	if ( l == k)
	printf ("Функція рефлективна!!!\n");
	if ( l != k) 
	printf ("Функція не рефлективна!!!\n");
return 0;
}
