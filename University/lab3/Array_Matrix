#include <stdio.h>
#include <iostream>
using namespace std;

int main(void)
{
	const int size = 5;
	int A[size];
	int B[size];
	int k = 0;
	int l = 0;
	printf("Enter the size of elements:");
	scanf("%i", &size);
	printf("Complete set of A:\n");


	for (int i = 0; i < size; i++)
		cin >> A[i];
	printf("Complete set of B:\n");
	for (int i = 0; i < size; i++)
		cin >> B[i];

	for (int i = 0; i < size; i++) {
		l += A[i];
		k += B[i];
	}
	if (l == k)
		printf("Reflective function !!!\n");
	if (l != k)
		printf("Function is not reflective !!!\n");
	return 0;
}
